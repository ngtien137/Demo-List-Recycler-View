<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.chimte.demomultilayoutwithconcatadapter.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_concatAdapterFragment"
            app:destination="@id/concatAdapterFragment" />
    </fragment>
    <fragment
        android:id="@+id/concatAdapterFragment"
        android:name="com.chimte.demomultilayoutwithconcatadapter.ui.concat_adapter.ConcatAdapterFragment"
        android:label="ConcatAdapterFragment"
        tools:layout="@layout/fragment_concat_adapter" />
</navigation>